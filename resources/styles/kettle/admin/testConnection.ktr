import java.sql.Connection;
import java.lang.Class;
import java.sql.DriverManager;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }
	try{
        Class.forName(get(Fields.In, "DB.DRIVER").getString(r));
		final String JDBCUrl = "jdbc:mysql://" + get(Fields.In, "DB.HOST").getString(r) + ":" + get(Fields.In, "DB.PORT").getString(r) + "/" +
			get(Fields.In, "DB.DATABASE").getString(r);
        final Connection conn = DriverManager.getConnection(JDBCUrl, get(Fields.In, "DB.USER").getString(r), get(Fields.In, "DB.PASS").getString(r));
		get(Fields.Out, "testConnectionResult").setValue(r, conn.isValid(get(Fields.In, "TIMEOUT.CONN").getInteger(r).intValue()));
	}catch(Exception e ){
		get(Fields.Out, "testConnectionResult").setValue(r, false);
	}
	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
