        <class_source><![CDATA[
import com.sun.jersey.api.client.Client;
import com.sun.jersey.api.client.ClientResponse;
import com.sun.jersey.api.client.WebResource;
import com.sun.jersey.api.client.filter.HTTPBasicAuthFilter;
import com.sun.jersey.core.header.FormDataContentDisposition;
import com.sun.jersey.multipart.FormDataMultiPart;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }
		
	final Client client = Client.create();
    client.addFilter(new HTTPBasicAuthFilter(get(Fields.In, "pentaho-server-username").getString(r), get(Fields.In, "pentaho-server-password").getString(r)));


	final WebResource deleteWebResource = client.resource(get(Fields.In, "pentaho-server-url").getString(r) + "plugin/data-access/api/connection/deletebyname?name=" + get(Fields.In, "DB.CONNECTION").getString(r));
	final ClientResponse deleteResponse = (ClientResponse)deleteWebResource.type("application/json").delete(ClientResponse.class);


	final WebResource webResource = client.resource(get(Fields.In, "pentaho-server-url").getString(r) + "plugin/data-access/api/connection/add");

