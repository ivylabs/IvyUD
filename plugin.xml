			<menubar id="toolsmenu">
	        	<menuitem id="IvyUD" label="${Launcher.IvyUD}" command="mantleXulHandler.openUrl('${Launcher.IvyUD}','${Launcher.IvyUD_TOOLTIP}','plugin/IvyUD/api/default')"/>
