			<title>IvyUD</title>
			<description></description>
			<company name="" url="" logo="@PLUGIN_COMPANY_LOGO@"></company>
		</content-type>
	</content-types>
	<content-generator
		id="IvyUD"
		title="IvyUD"
		type="IvyUD"
		class="pt.webdetails.cpk.CpkContentGenerator"/>
	<!-- Menu entry -->
	<menu-items>
		<!--<menu-item id="IvyUD_main" anchor="tools-submenu" label="IvyUD" command="content/IvyUD/" type="MENU_ITEM" how="LAST_CHILD"/>-->
